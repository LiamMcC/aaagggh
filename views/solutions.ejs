<%- include('../partials/header') -%>


    	<!-- start about -->
		<section id="about">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s"><span><%- (result[1][0].Title).toUpperCase() %></span></h2>
    				</div>
               
					<div class="col-md-12 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-offset="50" data-wow-delay="0.6s">
						<div class="media">
							<div class="media-heading-wrapper">
								<div class="media-object pull-left">
									<i class="fa fa-server"></i>
								</div>
								<h3 class="media-heading"><%- result[1][0].SmallBit %></h3>
							</div>
							<div class="media-body">
								<p>
									<% try { %>
										<%- result[1][0].Description %>
									  <% } catch (err) { %>
										<p>It looks as if there is an error with this section. We may not be aware of this so please let us know <a href="contact">Contact Us</a> Thank you!</p>
									  <% } %>
									

								</p>
							</div>
						</div>
					</div>

			
				</div>
			</div>
		</section>
		<!-- end about -->

  

    	<!-- start service -->
    	<section id="service">
    		<div class="container">
    			<div class="row">
    				<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s">PICK <span>ANY</span> PACKAGES</h2>
    				</div>
					<% try { %>
			
					<% result[0].forEach(function(product) { %>
						
    				<div class="col-md-4 wow fadeIn" data-wow-offset="50" data-wow-delay="0.6s" style="min-height: 490px;">
    					<i class="<%= product.Icon %>"></i>
    					<h4><%= product.Title %></h4>
    					<p>
							<%= product.Description.substring(0, 300) %> ...
							<br><br>&euro;<%= product.price %> Inc Vat
						</p>
						<h3><a href="/solution/<%= product.Title %>/<%= product.Id %>" style="text-decoration: none;">More</a></h3>
                        
						<% if (isAuthenticated) { %>

							<form action="/cart/<%= product.Id %>" method="post" name="productName" value="<%= product.Name %>">
								<input type="submit" class="form-control" value="Add To Cart &euro;<%= product.price %>" >
							</form>
						
						<%} else { %>
						  <a href="/login">LOG IN TO BUY</a>
						  
						  
					 
					  <% } %>


						
					
					



						</div>
											<% }); %>
    			
										
											<% } catch (err) { %>
											  <p>It looks as if there is an error with this section. We may not be aware of this so please let us know <a href="contact">Contact Us</a> Thank you!</p>
											<% } %>
    			</div>
    		</div>
    	</section>
    	<!-- end servie -->

    	
    




      

      <%- include('../partials/contact') -%>
      <%- include('../partials/footer') -%>
	  <%- include('../partials/cookie') -%>