<%- include('../partials/header') -%>
<%- include('../partials/banner') -%>

    	<!-- start about -->
		<section id="about">
			<div class="container">
				<div class="row">
					<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s">WHAT<span>AAAGGGH.COM </span> DO</h2>
    				</div>


					<% result[2].forEach(function(product) { %>      
					<div class="col-md-4 col-sm-4 col-xs-12 wow fadeInLeft" data-wow-offset="50" data-wow-delay="0.6s">
						<div class="media">
							<div class="media-heading-wrapper">
								<div class="media-object pull-left">
									<i class="fa fa-server"></i>
								</div>
								<h3 class="media-heading"><%= product.Title %></h3>
							</div>
							<div class="media-body">
								<p><%- product.Description.substring(0, 200) %>...</p>
							</div>
						</div>
					</div>
<% }); %>





	
				</div>
			</div>
		</section>
		<!-- end about -->

    	<!-- start team -->
    	<section id="team">
    		<div class="container">
    			<div class="row">
    				<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s">THE <span>AAAGGGH.COM</span> SOLUTION</h2>
    				</div>


                    <% result[0].forEach(function(product) { %>
		
    				<div class="col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-offset="50" data-wow-delay="1.3s">
    					<div class="team-wrapper" >
    						<img src="../<%= product.Image %>" class="img-responsive" alt="team img 1">
    							<div class="team-des" style="min-height:270px;">
    								<h4><%= product.Title %></h4>

								
    								<span>Server Side Code</span>
    								<p>
                                        <%= product.Description.substring(0, 200) %> 
                                    </p>
    							</div>
    					</div>
    				</div>
                    <% }); %>

    			</div>
    		</div>
    	</section>
    	<!-- end team -->

    	<!-- start service -->
    	<section id="service">
    		<div class="container">
    			<div class="row">
    				<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s">WHAT <span>AAAGGGH.COM</span> GIVES YOU </h2>
    				</div>

					<% result[1].forEach(function(product) { %>
    				<div class="col-md-4 wow fadeIn" data-wow-offset="50" data-wow-delay="0.6s">
    					<i class="<%= product.Icon %>"></i>
    					<h4><%= product.Title %></h4>
    					<p>
							<%= product.Description.substring(0, 300) %> ...
						</p>
                        </div>

						<% }); %>
    			

    			</div>
    		</div>
    	</section>
    	<!-- end servie -->

    	<!-- start portfolio -->
    	<section id="portfolio">
    		<div class="container">
    			<div class="row">
    				<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s"><span>STYLED</span> EXAMPLES</h2>
    				</div>

					<% result[3].forEach(function(product) { %>
    				<div class="col-md-3 col-sm-6 col-xs-12 wow fadeIn" data-wow-offset="50" data-wow-delay="0.6s">
                        <div class="portfolio-thumb">
							<img src="./resized/<%= product.Imageb %>" class="img-responsive" alt="team img 1">
                                <div class="portfolio-overlay">
                                    <h4><%= product.Title %></h4>
                                    <p><%= product.Description.substring(0, 80) %></p>
                                    <a href="/examples/<%= product.Id %>" class="btn btn-default">DETAIL</a>
                                </div>
                        </div>
    				</div>
    				<% }); %>
    			</div>
    		</div>
    	</section>
    	<!-- end portfolio -->

      

    	


		<%- include('../partials/contact') -%>
		<%- include('../partials/footer') -%>
		<%- include('../partials/cookie') -%>