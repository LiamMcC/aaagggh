<%- include('../partials/header') -%>


<section id="contact">
    <div class="container">
        <div class="row">

            <div class="col-lg-12 col-12">
              <h2 class="mb-4">Register Account</h2>
              <p>Welcome Along!</p>
            
            </div>



            <div class="col-lg-12 col-12">
                <div class="contact-form">
                  <h2 class="mb-4">Add A New About Section Here s</h2>
      
                  <form action="/signup" method="post">
                    <div class="row">
                      <div class="col-lg-6 col-12">
                        <input type="text" class="form-control"  placeholder="Name" name="username">
                      </div>
      
                      <div class="col-lg-6 col-12">
                        <input type="email" class="form-control"  placeholder="Email" name="email">
                      </div>
      
                      <div class="col-lg-6 col-12">
                        <input type="password" class="form-control"  placeholder="Password" name="password">
                      </div>

                    
      
                      <div class="ml-lg-auto col-lg-6 col-12">
                        <input type="submit" class="form-control submit-btn" value="Register">
                      </div>
                    </div>
                  </form>
                </div>
              </div>

        </div>
    </div>
</section>




<%- include('../partials/footer') -%>
<%- include('../partials/cookie') -%>
