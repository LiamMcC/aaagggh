<%- include('../partials/header') -%>




 

    	<!-- start about -->
		<section id="about">
			<div class="container">
                <% result.forEach(function(product) { %>




				<div class="row">
					<div class="col-md-12">
    					<h2 class="wow bounceIn" data-wow-offset="50" data-wow-delay="0.3s"><span><%= product.Title %></span></h2>
                        <div class="col-md-3 col-sm-4 col-xs-6" style="margin-bottom:20px; float: right;"></div>
                    </div>
                

					<div class="col-md-12 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-offset="50" data-wow-delay="0.6s">
						<div class="media">
                                <div class="media-heading-wrapper">
                                    <div class="media-object pull-left">
                                        <i class="<%= product.Icon %>"></i>
                                    </div>
                                    <h3 class="media-heading"><%= product.SmallBit %>
                                    </h3>
                                </div>
                                <div class="media-body">
                                    <p><%- product.Description %></p>  
                                    <p><img src="/<%= product.Image %>" class="img-responsive" alt="<%= product.ImageTitle %>"></p>
                                    <h3><%- product.ImageTitle %></h3>
                                </div>
						</div>
					</div>
                </div>





                <div class="row">
        		<div class="col-md-12 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-offset="50" data-wow-delay="0.6s">
						<div class="media">
                            <div class="media-heading-wrapper">
                                <div class="media-object pull-left">
                                    <i class="<%= product.Icon %>"></i>
                                </div>
                               
                            </div>
                                <div class="media-body">
                                    <p><%- product.Descriptionb %></p>  
                                    <p><img src="/<%= product.Imageb %>" class="img-responsive" alt="<%= product.ImageTitle %>"></p>
                                    <h3><%- product.ImageTitleb %></h3>
                                </div>
						</div>
					</div>
                </div>


                <div class="row">
                    <div class="col-md-12 col-sm-12 col-xs-12 wow fadeInLeft" data-wow-offset="50" data-wow-delay="0.6s">
                            <div class="media">
                                <div class="media-heading-wrapper">
                                    <div class="media-object pull-left">
                                        <i class="<%= product.Icon %>"></i>
                                    </div>
                                   
                                </div>
                                    <div class="media-body">
                                        <p><%- product.Descriptionc %></p>  
                                        <p><img src="/<%= product.Imagec %>" class="img-responsive" alt="<%= product.ImageTitle %>"></p>
                                        <h3><%- product.ImageTitlec %></h3>
                                    </div>
                            </div>
                        </div>
                    </div>
                    <% }); %>

				
			</div>
		</section>
		<!-- end about -->
   

 

   

    	


